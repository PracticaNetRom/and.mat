@model SummerCamp2017.Models.CloseAnnouncement
 

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @Html.HiddenFor(m => m.Id)

    <div class="form-group">
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Email)
           
            @Html.ValidationMessage("err")
        </div>
    </div>
    <button type="submit">Close annuncement</button>
}

@Html.ActionLink("Back to announcements list", "List")